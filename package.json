{
  "name": "@patrickkfkan/cross-spawn",
  "version": "7.1.0",
  "description": "Cross platform child_process#spawn and child_process#spawnSync",
  "keywords": [
    "spawn",
    "spawnSync",
    "windows",
    "cross-platform",
    "path-ext",
    "shebang",
    "cmd",
    "execute"
  ],
  "author": "Andr√© Cruz <andre@moxy.studio>",
  "homepage": "https://github.com/moxystudio/node-cross-spawn",
  "repository": {
    "type": "git",
    "url": "git@github.com:moxystudio/node-cross-spawn.git"
  },
  "license": "MIT",
  "main": "./dist/cjs/index-cjs.js",
  "module": "./dist/mjs/index.js",
  "types": "./dist/mjs/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/mjs/index.d.ts",
        "default": "./dist/mjs/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index-cjs.js"
      }
    }
  },
  "scripts": {
    "build": "npm run prepare",
    "build:esm": "npx tsc -p tsconfig-esm.json",
    "build:cjs": "npx tsc -p tsconfig.json",
    "prepare": "rm -rf dist && npm run build:esm && npm run build:cjs && bash fixup.sh",
    "lint": "npx eslint ./src && npx eslint ./test",
    "lint:fix": "npx eslint ./src --fix && npx eslint ./test --fix",
    "test": "jest --env node --coverage"
  },
  "dependencies": {
    "path-key": "^3.1.1",
    "read-cmd-shim": "^4.0.0",
    "shebang-command": "^2.0.0",
    "which": "^4.0.0"
  },
  "devDependencies": {
    "@types/shebang-command": "^1.2.2",
    "@types/which": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "eslint-plugin-jest": "^27.9.0",
    "jest": "^29.7.0",
    "mkdirp": "^3.0.1",
    "rimraf": "^5.0.7",
    "typescript": "<5.2.0"
  },
  "engines": {
    "node": ">= 16"
  }
}
